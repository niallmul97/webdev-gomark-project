{{#each bookmarklistcollection}}
  <section class="ui stacked segment">
    <h2 class="ui header">
      {{title}} ( {{bookmarks.length}} )
    </h2>
    <a href="/bookmarklist/{{id}}" class="ui icon button">
      <i class="icon folder open"></i>
    </a>
    <a href="/dashboard/deletebookmarklist/{{id}}" class="ui icon button">
      <i class="icon trash"></i>
    </a>
    <a href="/dashboard/makeCollectionPublic/{{id}}" class="ui icon button">
        <i class="unhide icon"></i>
    </a>
    <a href="/dashboard/makeCollectionPrivate/{{id}}" class ="ui icon button">
        <i class="hide icon"></i>
    </a>
  </section>
{{/each}}
